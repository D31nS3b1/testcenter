<Booklet
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../definitions/vo_Booklet.xsd"
>
  <Metadata>
    <Id>BOOKLET.SAMPLE</Id>
    <Label>Sample booklet</Label>
    <Description>This a a sample booklet for testing/development/showcase purposes.</Description>
    <Owner>__USER_NAME__</Owner>
    <Lastchange>2019-10-27T13:37:00</Lastchange>
    <Status>unknown</Status>
    <Project>sample</Project>
  </Metadata>

  <BookletConfig>
    <Config key="NextOnlyIfPresentationComplete"/>
    <Config key="NavPolicy"/>
    <Config key="NavButtons">on</Config>
    <Config key="PageNavBar">on</Config>
    <Config key="Logging">on</Config>
    <Config key="Loading">on</Config>
  </BookletConfig>

  <Units id="section_A" label="section_A">
    <Restrictions>
      <TimeMax parameter="string">60</TimeMax>
    </Restrictions>

    <Testlet>

      <Unit id="UNIT.SAMPLE" label="the great sample unit" labelshort="sample unit" />
    </Testlet>

    <Testlet>

      <Restrictions>
        <CodeToEnter parameter="sample">Please insert the word `sample`.</CodeToEnter>
        <TimeMax parameter="1" />
      </Restrictions>

      <Unit id="UNIT.SAMPLE-2" label="the great sample unit" labelshort="sample unit" />
    </Testlet>

  </Units>
</Booklet>
