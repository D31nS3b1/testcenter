{
  "name": "itc-ng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqb-berlin/testcenter-iqb-ng"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --configuration=local",
    "build": "ng build",
    "test": "ng test  --watch=false",
    "lint": "ng lint",
    "compodoc": "npx compodoc -p tsconfig.doc.json -s -d ./docs/compodoc",
    "update-compodoc": "npx compodoc -p tsconfig.doc.json -d ./docs/compodoc"
  },
  "private": true,
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "firefox esr"
  ],
  "dependencies": {
    "@angular/animations": "~14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "~14.2.12",
    "@angular/compiler": "~14.2.12",
    "@angular/core": "~14.2.12",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "~14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "~14.2.12",
    "@angular/platform-browser-dynamic": "~14.2.12",
    "@angular/router": "~14.2.12",
    "@types/ua-parser-js": "^0.7.36",
    "browserslist-useragent": "^4.0.0",
    "core-js": "^3.24.0",
    "fibers": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "material-design-icons": "~3.0.1",
    "qr-scanner": "1.4.1",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "ua-parser-js": "^1.0.35",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular-devkit/build-angular": "~14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "~14.2.12",
    "@angular/language-service": "~14.2.12",
    "@compodoc/compodoc": "^1.1.19",
    "@iqb/eslint-config": "2.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/jasminewd2": "~2.0.10",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "nyc": "15.1.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.4"
  },
  "resolutions": {
    "tree-kill": "1.2.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "**/*.spec.ts"
    ],
    "include": [
      "src"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "exclude-after-remap": false
  }
}
